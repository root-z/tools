		"test:watch": "vitest",
		"coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^22.15.21",
		"@vitest/coverage-v8": "^3.2.4",
		"jsdom": "^26.1.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.3.5",
		"vitest": "^3.1.4"
	},
	"dependencies": {
		"bootstrap": "^5.3.7",
		"bootswatch": "^5.3.7"
	}
